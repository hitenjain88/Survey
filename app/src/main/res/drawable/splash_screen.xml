<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="10dp"
                android:color="#01194E"/>
            <corners
                android:radius="30dp"/>
